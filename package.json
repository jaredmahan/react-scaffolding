{
  "name": "ems-react-test",
  "version": "1.0.0",
  "description": "ems-react-test project",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server  --progress --config config/webpack.config.dev.js",
    "test": "jest --no-cache",
    "test:watch": "npm run test -- --watch",
    "test:update": "npm test -- u",
    "build": "rimraf dist && webpack --config config/webpack.config.prod.js --progress --profile --bail"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/typescript-babel-jest/index.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "(/__test__/.*|\\.spec)\\.(ts|tsx|js)$"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/ianems/ems-react-test.git"
  },
  "author": "Jared Mahan",
  "license": "ISC",
  "homepage": "https://bitbucket.org/ianems/ems-react-test#readme",
  "dependencies": {
    "webpack": "^1.13.3",
    "webpack-notifier": "^1.4.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "0.0.31",
    "@types/jest": "^16.0.4",
    "@types/moment": "^2.13.0",
    "@types/moment-duration-format": "^1.3.1",
    "@types/react": "^0.14.51",
    "@types/react-addons-test-utils": "^0.14.16",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "@types/react-tap-event-plugin": "0.0.30",
    "@types/redux-thunk": "^2.1.0",
    "awesome-typescript-loader": "^3.0.0-beta.9",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "connect": "^3.5.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^18.1.0",
    "json-loader": "^0.5.4",
    "moment": "^2.17.0",
    "moment-duration-format": "^1.3.0",
    "rc-calendar": "^7.5.1",
    "react": "^15.4.1",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "^4.4.6",
    "react-tap-event-plugin": "^2.0.1",
    "react-test-renderer": "^15.4.2",
    "redux": "^3.6.0",
    "redux-devtools": "^3.3.1",
    "redux-thunk": "^2.1.0",
    "serve-static": "^1.11.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "tslint": "^4.0.2",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.0.10",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.3.1"
  }
}
